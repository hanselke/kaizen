<!doctype html>
<html xmlns:ng="http://angularjs.org/">
<head>
  <meta charset="utf-8">
  <title>Open Business</title>
  <link rel="stylesheet" href="css/app.css" media="screen"/>
  <link rel="stylesheet" href="css/print.css" media="print"/>
  <script src="/lib/modernizr.js"></script>

</head>
<body ng:controller="AppController">
  <div class="help overlay" onclick="$('.help').hide()"></div>
  <div class="noPrint" style="position:fixed; width:100%; height:22px; bottom:0px; text-align: right;"><div style="display: inline-block;height:22px;padding:3px 4px;margin-right: 282px;border-radius: 5px 5px 0 0; color:#aaa; background: url('img/background-small.png');cursor:pointer;font-family: Helvetica;font-size: 14px" onclick="$('.help').show()">Help</div></div>
  <table ng:show="currentUser" style="display:none">
    <tr style="height: 10px">
      <td style="width: 100px" ng:show="currentUser" class="noPrint">
        <div style="background-color: white; border-radius: 7px;color:black" class="fullheight">
          <div style="padding: 7px 5px">
            <a href="#"><img src="img/logo.png" style="width: 110px"/></a>
          </div>
          <div style="padding-left:5px">
            <button style="margin-top:1em;margin-bottom:1em;font-weight:bold;height:2em;color:blue" ng:click="nextTask()">Next action</button><br>
          </div>
          <div style="padding:5px 5px 1px 5px">
            <table><tr><td>
              <a href="/users/sign-out">
                <img ng:src="{{currentUser.selectedUserImage}}" style="border-radius: 5px;height:50px;width:50px" ng:show="currentUser.selectedUserImage"/>
                <img ng:src="/img/placeholders/avatar-50x50.jpg" style="border-radius: 5px;height:50px;width:50px" ng:show="!currentUser.selectedUserImage"/>
              </a>
            </td><td>{{currentUser.username}}</td></tr></table>
          </div>
          <div class="process_history" style="padding: 5px" ng:show="process_history">
            <div class="box">
              <div class="title">RFQ to Quote</div>
              <div class="text" style=" background-color: #B6CDE4;">Mike</div>
              <div class="box" ng:repeat="p in process_history">
                <div class="title">{{p[0]}}</div>
                <div class="text">{{p[1]}}</div>
              </div>
            </div>
          </div>
          <div class="help bubble left">These are a list of tasks<br>only you can access</div>
          <div class="process_history" style="padding: 5px" ng:show="tasks">
            <div class="box">
              <div class="title">Personal Tasks</div>
              <div class="box" ng:repeat="p in tasks">
                <div class="title">{{p[0]}}</div>
                <div class="text" ng:repeat="t in p[1]">{{t}}</div>
              </div>
            </div>
          </div>
          <div class="help bubble left">These are a list of tasks the<br>entire sales team can access</div>
          <div class="process_history" style="padding: 5px" ng:show="tasks">
            <div class="box">
              <div class="title">Sales department</div>
              <div class="box">
                <div class="text" ng:repeat="t in dept_tasks">{{t}}</div>
              </div>
            </div>
          </div>
        </div>
      </td>
      <td style="height: 100%">
        <ng:view style="height: 100%; display: block;"></ng:view>
      </td>
    </tr>
  </table>
  <div style="text-align: center; display: none" ng:show="currentUser == null">
    <ng:include src="'signin.html'" ng:controller="SigninController"></ng:include>
  </div>
